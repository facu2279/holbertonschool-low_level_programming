#include <stdio.h>
#include "dog.h"
/**
 * print_dog - asda
 * @d: asdf
 */
void print_dog(struct dog *d)
{
	if (d != 0)
	{
		if (name == 0)
		{
			printf("Name: (nil)\n");
		}
		else
		{
			printf("Name: %s\n", d->name);
		}
		if (age == 0)
		{
			printf("Age: (nil)\n");
		}
		else
		{
			printf("Age: %f\n", d->age);
		}
		if (owner == 0)
		{
			printf("Owner: (nil)\n");
		}
		else
		{
			printf("Owner: %s\n", d->owner);
		}
	}
}
